   "metadata": {},
   "source": [
    "# Calculate the number of orders per month in 2018."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 102,
   "id": "86cb4171-b5e6-4aa7-9fc7-729e82ed45cb",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 640x480 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "query = \"\"\"\n",
    "SELECT MONTHNAME(order_purchase_timestamp) AS month, COUNT(order_id) AS order_count\n",
    "FROM orders\n",
    "WHERE YEAR(order_purchase_timestamp) = 2018\n",
    "GROUP BY MONTHNAME(order_purchase_timestamp)\n",
    "\"\"\"\n",
    "\n",
    "cur.execute(query)\n",
    "\n",
    "data = cur.fetchall()\n",
    "df=pd.DataFrame(data, columns=[\"months\",\"order_count\"])\n",
    "o= ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August','September', 'October']\n",
    "\n",
    "ax= sns.barplot(x=\"months\", y=\"order_count\", data=df, order=o, hue = df[\"months\"], palette=\"viridis\")\n",
    "plt.xticks(rotation=90)\n",
    "ax.bar_label(ax.containers[0])\n",
    "plt.title(\"Count of Orders by Months in 2018\")\n",
    "plt.show()"
   ]
  },
  {
