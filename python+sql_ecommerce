    "# Count the number of customers from each state. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 100,
   "id": "5d6074b0-dda9-4f90-92ec-57120f12844f",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "<function matplotlib.pyplot.show(close=None, block=None)>"
      ]
     },
     "execution_count": 100,
     "metadata": {},
     "output_type": "execute_result"
    },
    {
     "data": {
      "image/png": "",
      "text/plain": [
       "<Figure size 800x300 with 1 Axes>"
      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "query = \"\"\"\n",
    "            SELECT customer_state, COUNT(customer_id) \n",
    "            FROM customers\n",
    "            GROUP BY customer_state\n",
    "            \"\"\"\n",
    "\n",
    "cur.execute(query)\n",
    "\n",
    "data = cur.fetchall()\n",
    "\n",
    "df=pd.DataFrame(data, columns= [\"State\", \"Customer_Count\"])\n",
    "df\n",
    "\n",
    "df=df.sort_values(by=\"Customer_Count\", ascending=False)\n",
    "\n",
    "plt.figure(figsize =(8,3))\n",
    "plt.bar(df[\"State\"], df[\"Customer_Count\"])\n",
    "plt.xticks(rotation=90)\n",
    "plt.xlabel(\"states\")\n",
    "plt.ylabel(\"Customer_Count\")\n",
    "plt.title(\"Count of Customers by State\")\n",
    "\n",
    "plt.show"
   ]
