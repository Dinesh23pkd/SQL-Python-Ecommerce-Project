    "# Calculate the percentage of orders that were paid in installments."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "id": "4e5b732b-3fc3-4304-9349-71fb1ab62f6d",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "('the percentage of orders that were paid in installments', Decimal('99.9981'))"
      ]
     },
     "execution_count": 49,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "query = \"\"\"\n",
    "            SELECT (SUM(CASE WHEN payment_installments >= 1 THEN 1 ELSE 0 END)) / COUNT(*) * 100 AS percentage_installments\n",
    "            FROM payments;\n",
    "            \"\"\"\n",
    "\n",
    "cur.execute(query)\n",
    "\n",
    "data = cur.fetchall()\n",
    "\n",
    "\"the percentage of orders that were paid in installments\", data [0][0]\n"
   ]
  },
